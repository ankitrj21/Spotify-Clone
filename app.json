{
  "expo": {
    "name": "Expo Spotify",
    "description": "Spotify: UI Clone with React Native & Expo",
    "scheme": "spotifyclone",
    "slug": "spotifyclone",
    "privacy": "public",
    "version": "0.5.0",
    "platforms": ["android", "ios", "web"],
    "githubUrl": "https://github.com/ankitrj21/Spotify-Clone",
    "orientation": "portrait",
    "primaryColor": "#181414",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#181414",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "versionCode": 50
    },
    "ios": {
      "buildNumber": "0.5.0",
      "supportsTablet": true
    }
  }
}
